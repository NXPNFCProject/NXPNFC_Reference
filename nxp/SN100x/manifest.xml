    <hal format="hidl">
        <name>android.hardware.nfc</name>
        <transport>hwbinder</transport>
        <impl level="generic"></impl>
        <version>1.1</version>
        <version>2.0</version>
        <interface>
            <name>INfc</name>
            <instance>default</instance>
            <instance>nfc_nci</instance>
        </interface>
    </hal> -->
    <hal format="hidl">
        <name>vendor.nxp.nxpnfc</name>
        <transport>hwbinder</transport>
        <impl level="generic"></impl>
        <version>1.0</version>
    </hal>
    <hal format="hidl">
        <name>vendor.nxp.nxpese</name>
        <transport>hwbinder</transport>
        <impl level="generic"></impl>
        <version>1.0</version>
    </hal>
    <hal format="hidl">
        <name>vendor.nxp.nxpwiredse</name>
        <transport>hwbinder</transport>
        <impl level="generic"></impl>
        <version>1.0</version>
    </hal>
    <sepolicy>
        <version>0.0</version>
    </sepolicy>
</manifest>
