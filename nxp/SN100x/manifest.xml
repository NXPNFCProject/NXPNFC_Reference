<manifest version="1.0" type="device" >
    <hal format="hidl">
        <name>android.hardware.nfc</name>
        <transport>hwbinder</transport>
        <impl level="generic"></impl>
        <version>1.1</version>
        <interface>
            <name>INfc</name>
            <instance>default</instance>
            <instance>nfc_nci</instance>
        </interface>
    </hal>
    <hal format="hidl">
        <name>android.hardware.secure_element</name>
        <transport>hwbinder</transport>
        <impl level="generic"></impl>
        <version>1.0</version>
        <interface>
            <name>ISecureElement</name>
            <instance>eSE2</instance>
        </interface>
    </hal>
    <hal format="hidl">
        <name>android.hardware.wired_se</name>
        <transport>hwbinder</transport>
        <impl level="generic"></impl>
        <version>1.0</version>
        <interface>
            <name>INxpWiredSe</name>
            <instance>default</instance>
        </interface>
    </hal>
    <hal format="hidl">
        <name>android.hardware.trusted_se</name>
        <transport>hwbinder</transport>
        <impl level="generic"></impl>
        <version>1.0</version>
        <interface>
            <name>ISecureElement</name>
            <instance>eSE4</instance>
        </interface>
    </hal>
